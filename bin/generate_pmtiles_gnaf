#!/bin/sh
gunzip -k gnaf.$1.geojson.gz
tippecanoe -zg -ab -o gnaf.$1.pmtiles -l "au_gnaf_$1" -n "Geocoded National Address File ($1)" -A "Incorporates or developed using G-NAF Â© Geoscape Australia licensed by the Commonwealth of Australia under the Open Geo-coded National Address File (G-NAF) End User Licence Agreement." -Z16 -z16 -pk -d --read-parallel gnaf.$1.geojson
gzip gnaf.$1.pmtiles